<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--  Patient details Menu -->
    <menuitem id="hospital_management" name="Hospital Management" sequence="1">
        <menuitem id="patient_details" name="Patient Details">
            <menuitem id="patient_action" action="patient_card_action"/>
        </menuitem>
        <!-- Patient OP Menu-->
        <menuitem id="patient_op" name="OP" action="patient_op_action"/>

        <!-- Consultation Menu -->
        <menuitem id="patient_consultation" name="Consultation" action="consultation_action"/>

        <!-- Appointment Menu-->
        <menuitem id="patient_appointment" name="Appointment" action="patient_appointment_action"/>

        <!-- Hospital reporting Menu -->
        <menuitem id="hospital_report" name="Hospital Report">
            <menuitem id="patient_report" action="action_patient_report_wizard"/>
        </menuitem>

    </menuitem>
</odoo>