<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!--  creating reference number-->
    <data>
        <record id="patient_sequence" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">patient.sequence</field>
            <field name="prefix">PID</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <!--  Patient OP token no -->
    <data>
        <record id="patient_token_no" model="ir.sequence">
            <field name="name">OP Token NO</field>
            <field name="code">OP.Token</field>
            <field name="prefix">OP</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>